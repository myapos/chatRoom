{
  "name": "websocketsdemo",
  "version": "1.0.0",
  "description": "A server-client demo for implementing websockets protocol",
  "main": "server/server.js",
  "scripts": {
    "automate": "nodemon --exec babel-node ./server/server.js",
    "start": "webpack-dev-server --mode development --hot",
    "build": "webpack --config webpack.config.production.js",
    "test": "jest",
    "lint": "eslint"
  },
  "lint-staged": {
    "client/**/*.js": "eslint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test && yarn build"
    }
  },
  "keywords": [
    "websockets",
    "node",
    "express",
    "socket.io",
    "react",
    "redux"
  ],
  "author": "Myron Apostolakis",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.3.4",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.0",
    "core-js": "^2.6.5",
    "css-loader": "^2.1.1",
    "express": "^4.16.4",
    "file-loader": "^3.0.1",
    "http": "^0.0.0",
    "jest": "^24.5.0",
    "nodemon": "^1.18.10",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "remove": "^0.1.5",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "devDependencies": {
    "eslint": "^5.15.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5"
  }
}
