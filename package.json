{
  "name": "websocketsdemo",
  "version": "1.0.0",
  "description": "A server-client demo for implementing websockets protocol",
  "main": "src/server/server.js",
  "scripts": {
    "server": "nodemon --exec babel-node ./src/server/server.js",
    "client": "webpack-dev-server --mode development --hot",
    "build": "webpack --config webpack.config.production.js",
    "test": "NODE_ENV=production jest --maxWorkers=2",
    "testDev": "NODE_ENV=development jest --maxWorkers=2",
    "startAll_": "./startAll.sh",
    "startDev": "yarn client & yarn server",
    "start": "yarn server",
    "visualize": "depcruise --exclude \"^node_modules|test|fontawesome\" --output-type dot src | dot -T svg > ./graph/dependencygraph.svg",
    "lint": "eslint",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "src/client/**/*.js": "eslint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  },
  "keywords": [
    "websockets",
    "node",
    "express",
    "socket.io",
    "react",
    "redux"
  ],
  "author": "Myron Apostolakis",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.~4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.4.0",
    "@emotion/core": "^10.0.0",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.0",
    "css-loader": "^2.1.1",
    "express": "^4.16.4",
    "file-loader": "^3.0.1",
    "http": "^0.0.0",
    "nodemon": "^1.18.10",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-bootstrap-table": "^4.3.1",
    "react-dom": "^16.8.4",
    "react-loader-spinner": "^2.3.0",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.2",
    "remove": "^0.1.5",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.0.5",
    "@storybook/addon-links": "^5.0.5",
    "@storybook/addons": "^5.0.5",
    "@storybook/react": "^5.0.5",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "dependency-cruiser": "^4.15.0",
    "enzyme": "^3.9.0",
    "eslint": "^5.15.3",
    "eslint-config-defaults": "^9.0.0",
    "eslint-config-phaistos-networks": "^1.9.10",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^21.24.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.5.0",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "jest-css-modules": "^1.1.0",
    "jest-haste-map": "^24.5.0",
    "jest-puppeteer": "^4.1.0",
    "jest-resolve": "^24.5.0",
    "lint-staged": "^8.1.5",
    "puppeteer": "^1.13.0",
    "react-test-renderer": "^16.8.4"
  }
}
