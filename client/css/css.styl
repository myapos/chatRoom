* {
  margin:0;
  padding: 0;
}

body {
  background-color: gray;
}
.wrapper {
  width: 50%;
  margin: 0 auto;
  border: 1px solid #eee;
  background-color: lightgrey
}


.display {
  background-color: white;
  min-height: 400px;
  border:1px solid darkgray;
}
.myBtn {
  border:1px solid #ccc;
  background-color: gray;
  border-radius: 10px;
  cursor: pointer;
  text-align: center;
  margin: 0 auto;
  width: 10%;
  display: inline-block;
  float:  left;
}

.entry {
  width: 80%;
  float: left;
  display: inline-block;
}

.entryWrapper {
  width: 100%;
  display: inline-block;
}